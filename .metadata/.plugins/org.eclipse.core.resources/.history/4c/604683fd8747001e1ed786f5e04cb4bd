package miit100programs;

public class EvenOrOdd {

	public static void main(String[] args)  
	{
		int x = 7;
		int y = 10;
		if (x % 5 == 0) {
			System.out.println("Even");
			
		} else {
			System.out.println("Odd");
		}if (y % 3 == 0) {
			System.out.println("Even");
			
		} else {
			System.out.println("Odd");
		}
		
		
	

	}

}
